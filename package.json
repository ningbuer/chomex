{
  "name": "chomex",
  "version": "1.0.9",
  "description": "Chrome Extension Routing Kit",
  "main": "lib/chomex.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "release": "./node_modules/.bin/coffee ./scripts/release.coffee",
    "test": "eslint src && eslint tests && jest"
  },
  "repository": "https://github.com/otiai10/chomex.git",
  "keywords": [
    "chrome"
  ],
  "author": "otiai10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otiai10/chomex/issues"
  },
  "homepage": "https://github.com/otiai10/chomex#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coffee-script": "^1.12.7",
    "eslint": "^4.7.2",
    "jest-cli": "^21.1.0",
    "shelljs": "^0.7.8",
    "should": "^13.1.0",
    "webpack": "^3.6.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": "tests/.*-test\\.js$",
    "unmockedModulePathPatterns": [
      "<rootDir>/src/chomex"
    ],
    "setupTestFrameworkScriptFile": "./tests/mock/setup-script.js",
    "setupFiles": [
      "./tests/mock/chrome.js"
    ]
  }
}
